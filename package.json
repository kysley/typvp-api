  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.6",
    "graphql": "^14.5.8",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
