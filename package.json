{
  "name": "typvp-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/kysley/typvp-api.git",
  "author": "Evan Kysley <evan@kysley.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly ./src",
    "start": "node dist/index.js",
    "build": "rm -rf dist && tsc",
    "seed": "ts-node prisma/seed.ts",
    "start:docker": "docker-compose -f 'prisma/docker-compose.yml' up",
    "stop:docker": "docker-compose -f 'prisma/docker-compose.yml' down"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "graphql-shield": "^6.0.4",
    "graphql-yoga": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "prisma-client-lib": "^1.34.3",
    "rword": "^3.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^12.7.0",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "graphql": "^14.0.0"
  }
}
